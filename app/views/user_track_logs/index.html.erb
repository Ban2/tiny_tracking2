<p id="notice"><%= notice %></p>

<h1>User Track Logs</h1>

<table>
  <thead>
    <tr>
      
      <th>User name</th>
      <th>Arrival Time</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @user_track_logs.each do |user_track_log| %>
      <tr>
       
        <td><%= user_track_log.user_name %></td>
         <td><%= user_track_log.arrival_time.strftime('%a, %d %b %Y %H:%M:%S') unless user_track_log.arrival_time.blank? %></td>
        <td><%#= link_to 'Show', user_track_log %></td>
        <td><%#= link_to 'Edit', edit_user_track_log_path(user_track_log) %></td>
        <td><%= link_to 'Destroy', user_track_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate   @user_track_logs %>

